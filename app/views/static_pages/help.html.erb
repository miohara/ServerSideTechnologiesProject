<% provide(:title,"Contact") %>

<h1>Leave Us A Message!</h1>
<p>You can contact us below and also find our location</p>

<div id ="form">

<form action="" method="post">
		<fieldset>
            <div>
                <input type="text" name="first_name" id="fname" placeholder="First Name"/>
            </div>
            <div>
                <input type="text" name="last_name" id="lname" placeholder="Last Name"/>
            </div>
            <div>
                <input type="text" name="email" id="email" placeholder="Email"/>
            </div>
            <div>
                <input type="password" name="pass" id="password" placeholder="Password"/>
            </div>
            <div>
                <div class="small">Leave us a Message</div>
                    <textarea name="message" id="message" placeholder="Message"></textarea>
            </div>    
            <input type="button" value="Submit" onclick="emailValidator();">
        </fieldset>    
    </form>
</div>
	<script src="form.js"></script>
	
